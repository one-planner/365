package com.planner.domain;

import jakarta.persistence.Entity;
import jakarta.persistence.Id;
import lombok.Data;

@Data
@Entity
public class Planner {
	@Id
	private Long pNo;
	private String title;
	private String content;
	private String place;
	private int alarm;
	private int timeZone;
	private int repeat;
	
	
	
}
